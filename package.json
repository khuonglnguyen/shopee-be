    "egg-cors": "^2.2.3",
    "egg-jwt": "^3.1.7",
    "egg-router-plus": "^2.0.0",
    "egg-scripts": "^2.6.0",
    "egg-security": "^2.11.0",
    "egg-sequelize": "^6.0.0",
    "egg-validate": "^2.0.2",
    "mysql2": "^2.3.3",
    "path": "^0.12.7",
    "stream": "0.0.2",
    "util": "^0.12.5"
